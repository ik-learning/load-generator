    "requests_count": 350,
    "warmup_requests_count": 50,
    "timeout": 2,
    "path": "Live",
    "method": "post",
    "schemas": {
      "post": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "required": "true"
          },
          "date": {
            "type": "string",
            "required": "true"
          },
          "requests_sent": {
            "type": "number",
            "required": "true"
          }
        }
      },
      "200": {
        "type": "object",
        "properties": {
          "successful": {
            "type": "bool",
            "required": "true"
          }
        }
      }
    }
  }
}
